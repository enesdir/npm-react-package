{
	"name": "npm-react-package",
	"version": "0.0.1",
	"description": "npm package template with tsup and typescript",
	"repository": {
		"type": "git",
		"url": "https://github.com/[USERNAME]/my_package_name.git"
	},
	"homepage": "https://[USERNAME].github.io/my_package_name/",
	"bugs": {
		"url": "https://github.com/[USERNAME]/my_package_name/issues"
	},
	"author": "enesdir",
	"contributors": [
		{
			"name": "Your name",
			"email": "yourmail@mail.com"
		}
	],
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"react",
		"component",
		"tiny",
		"lightweight"
	],
	"scripts": {
		"build": "tsup ./src/index.ts",
		"build-storybook": "build-storybook",
		"dev": "tsup ./src/index.ts --watch",
		"lint": "eslint src --fix --cache",
		"lint-staged": "lint-staged",
		"prepare": "husky install",
		"push-release": "git push --follow-tags origin main",
		"release": "dotenv -e HUSKY=0 commit-and-tag-version",
		"size": "size-limit",
		"storybook": "storybook dev -p 6006",
		"test": "vitest"
	},
	"peerDependencies": {
		"react": "^16 || ^17 || ^18",
		"react-dom": "^16 || ^17 || ^18"
	},
	"dependencies": {},
	"devDependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@commitlint/cz-commitlint": "^18.4.3",
		"@commitlint/prompt-cli": "^18.4.3",
		"@ianvs/prettier-plugin-sort-imports": "^4.1.1",
		"@size-limit/preset-small-lib": "^11.0.1",
		"@storybook/addon-essentials": "^7.6.6",
		"@storybook/addon-interactions": "^7.6.6",
		"@storybook/addon-links": "^7.6.6",
		"@storybook/blocks": "^7.6.6",
		"@storybook/builder-vite": "^7.6.6",
		"@storybook/react": "^7.6.6",
		"@storybook/react-vite": "^7.6.6",
		"@storybook/testing-library": "^0.2.2",
		"@testing-library/react": "^14.1.2",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"commit-and-tag-version": "^12.0.0",
		"commitizen": "^4.3.0",
		"commitlint": "^18.4.3",
		"dotenv-cli": "^7.3.0",
		"eslint": "8.56.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^8.0.3",
		"jsdom": "^23.0.1",
		"lint-staged": "^15.2.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-jsdoc": "^1.3.0",
		"prettier-plugin-pkg": "^0.18.0",
		"prettier-plugin-sh": "^0.13.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-test-renderer": "18.2.0",
		"size-limit": "^11.0.1",
		"storybook": "^7.6.6",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.1.1"
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"maintained node versions"
	],
	"size-limit": [
		{
			"path": "dist/index.js",
			"limit": "10 KB"
		}
	]
}
